   year = {2003},
}
@article{Loscocco2001,
   abstract = {The protection mechanisms of current mainstream op- erating systems are inadequate to support confidentiality and integrity requirements for end systems. Mandatory access control (MAC) is needed to address such require- ments, but the limitations of traditional MAC have in- hibited its adoption into mainstream operating systems. The National Security Agency (NSA) worked with Se- cure Computing Corporation (SCC) to develop a flexi- ble MAC architecture called Flask to overcome the lim- itations of traditional MAC. The NSA has implemented this architecture in the Linux operating system, produc- ing a Security-Enhanced Linux (SELinux) prototype, to make the technology available to a wider community and to enable further research into secure operating sys- tems. NAI Labs has developed an example security pol- icy configuration to demonstrate the benefits of the ar- chitecture and to provide a foundation for others to use. This paper describes the security architecture, security mechanisms, application programming interface, secu- rity policy configuration, and performance of SELinux.},
